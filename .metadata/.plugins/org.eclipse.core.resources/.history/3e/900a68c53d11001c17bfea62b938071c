package ch03;

public class Activity1 extends BaseActivity {

	public Activity1(String name) {
		this.name = name;
		initData();
		setInitLayout();
		addEventListener();
		
	}
	
	@Override
	public void initData() {
		setTitle(name);
		
	}
	
	@Override
	public void setInitLayout() {
		// TODO Auto-generated method stub
		super.setInitLayout();
	}
	
	@Override
	public void addEventListener() {
		// TODO Auto-generated method stub
		super.addEventListener();
	}
	
}
