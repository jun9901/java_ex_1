package ch02;

import java.io.FileInputStream;

public class FileInputStreamTest3 {

	public static void main(String[] args) {
		try(FileInputStream fis = new FileInputStream("assets/input.txt")){
			int i;
			while((i = fis.read()) != -1) {
				System.out.println(i);
			}
		} catch(Exception e) {
			System.out.println(e);
			
		}
	}

}
