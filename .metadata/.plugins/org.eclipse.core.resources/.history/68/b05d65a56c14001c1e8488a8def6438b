package ch05;

public class SerializationTest {

	public static void main(String[] args) {

		Person personLee = new Person("이순신", "대표이사");
		Person personKim = new Person("김유신", "상무이사");
	}

}

class Person{
	String name;
	String job;
	
	public Person() {
		
	}
	
	public Person(String name, String job) {
		this.name = name;
		this.job = job;
	}
	
	@Override
	public String toString() {
		return name + ", " + job;
	}
}
