package ch01;

import java.util.HashSet;

public class DataStructureTest1 {

	public static void main(String[] args) {

		HashSet<Integer> set1 = new HashSet<>();
		set1.add(1);
		set1.add(1); // 중복 값
		set1.add(2);
		set1.add(3);
		set1.add(3);
		set1.add(3);
		
		// 사이즈 확인
		System.out.println(set1.size());
		
		set1.remove(1);
		System.out.println(set1);
	}

}
